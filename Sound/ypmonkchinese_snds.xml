<protounitsounddef>
  <protounit name="ypMonkChinese">
    <soundtype name="Select">
      <skinlogic>
        <choice name="none">
          <soundset name="ChineseMonkSelect">
          </soundset>
        </choice>
        <choice name="0">
          <soundset name="ChineseChenSelect">
          </soundset>
        </choice>
        <choice name="1">
          <soundset name="ChineseHuangSelect">
          </soundset>
        </choice>
        <choice name="2">
          <soundset name="ChineseNunSelect">
          </soundset>
        </choice>
      </skinlogic>
    </soundtype>
    <soundtype name="Grunt">
      <skinlogic>
        <choice name="none">
          <soundset name="GenericMaleGrunt">
          </soundset>
        </choice>
        <choice name="2">
          <soundset name="GenericFemaleGrunt">
          </soundset>
        </choice>
      </skinlogic>
    </soundtype>
    <soundtype name="Death">
      <skinlogic>
