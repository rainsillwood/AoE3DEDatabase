<protounitsounddef>
  <protounit name="deREVCetbang">
    <soundtype name="Select">
      <civlogic>
        <choice name="Dutch">
          <soundset name="DutchFalconetSelect">
          </soundset>
        </choice>
        <choice name="Portuguese">
          <soundset name="PortugueseFalconetSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="Dutch">
          <targetlogic>
            <choice name="default">
              <soundset name="DutchFalconetAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="DutchFalconetAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
        <choice name="Portuguese">
          <targetlogic>
            <choice name="default">
              <soundset name="PortugueseFalconetAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="PortugueseFalconetAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddef>