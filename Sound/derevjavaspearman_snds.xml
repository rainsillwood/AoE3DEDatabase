<protounitsounddef>
  <protounit name="deREVJavaSpearman">
    <soundtype name="Select">
      <civlogic>
        <choice name="Dutch">
          <soundset name="DutchPikemanSelect">
          </soundset>
        </choice>
        <choice name="Portuguese">
          <soundset name="PortuguesePikemanSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="Dutch">
          <targetlogic>
            <choice name="default">
              <soundset name="DutchPikemanAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="DutchPikemanAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
        <choice name="Portuguese">
          <targetlogic>
            <choice name="default">
              <soundset name="PortuguesePikemanAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="PortuguesePikemanAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddef>