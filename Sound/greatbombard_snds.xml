<protounitsounddef>
  <protounit name="GreatBombard">
    <soundtype name="Select">
      <civlogic>
        <choice name="Ottomans">
          <soundset name="OttomanGreatBombardSelect">
          </soundset>
        </choice>
        <choice name="Russians">
          <soundset name="RussianCannonSelect">
          </soundset>
        </choice>
      </civlogic>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="MilitaryBirth">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <civlogic>
        <choice name="Ottomans">
          <targetlogic>
            <choice name="default">
              <soundset name="OttomanGreatBombardAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="OttomanGreatBombardAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
        <choice name="Russians">
          <targetlogic>
            <choice name="default">
              <soundset name="RussianCannonAcknowledge">
              </soundset>
            </choice>
            <choice name="enemy">
              <soundset name="RussianCannonAttack">
              </soundset>
            </choice>
          </targetlogic>
        </choice>
      </civlogic>
    </soundtype>
  </protounit>
</protounitsounddef>