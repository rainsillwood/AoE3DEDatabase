<protounitsounddef>
  <protounit name="dePadre">
    <soundtype name="Select">
      <soundset name="DEMexicanPadreSelect">
      </soundset>
    </soundtype>
    <soundtype name="Grunt">
      <soundset name="GenericMaleGrunt">
      </soundset>
    </soundtype>
    <soundtype name="Death">
      <soundset name="GenericMaleDeath">
      </soundset>
    </soundtype>
    <soundtype name="Creation">
      <soundset name="DEMexicanPadreRevived">
      </soundset>
    </soundtype>
    <soundtype name="Acknowledge">
      <targetlogic>
        <choice name="default">
          <soundset name="DEMexicanPadreAcknowledge">
          </soundset>
        </choice>
        <choice name="enemy">
          <soundset name="DEMexicanPadreAttack">
          </soundset>
        </choice>
      </targetlogic>
    </soundtype>
    <soundtype name="KnockOut">
      <soundset name="DEMexicanPadreDisabled">
      </soundset>
    </soundtype>
    <soundtype name="KnockOutRevived">
      <soundset name="DEMexicanPadreRevived">
      </soundset>
    </soundtype>
    <soundtype name="Ransomed">
      <soundset name="DEMexicanPadreRevived">
      </soundset>
    </soundtype>
  </protounit>
</protounitsounddef>