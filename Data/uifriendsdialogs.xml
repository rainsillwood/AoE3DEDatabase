    <button>OK</button>
    <button>$$18466$$Cancel</button>
    <command>gadgetUnreal("mpfriend-adddialog")</command>
  </gadget>
  <gadget type="dialog" name="mpfriend-addfriendrequestdialog" titlebar="Friend Add Request" size1024="0 0 400 130" bevel="" hidden="" a-dtype="textFontSize number Z number" centervert="" centerhoriz="" closebutton="">
    <gadget name="mpfriend-addfriendrequestdialog-prompt" type="textBox" sizerel1024="10 10 390 35" textcolor="0 0 0" textfont="Arial nobold nodrop" textfontsize="12" textcentervert="">Add %s as a friend?</gadget>
    <gadget name="mpfriend-addfriendrequestdialog-addtolist" type="checkButton" sizerel1024="10 40 390 65" textfont="Arial nodrop" textfontsize="12" configindex="whatever">Add them to my friends list</gadget>
    <button>Accept</button>
    <button>Reject</button>
    <command>gadgetUnreal("mpfriend-addfriendrequestdialog")</command>
  </gadget>
  <gadget type="dialog" name="mpfriend-removedialog" titlebar="Remove Friend" size1024="0 0 400 130" bevel="" hidden="" a-dtype="textFontSize number Z number" centervert="" centerhoriz="" closebutton="">
    <gadget name="mpfriend-removedialog-prompt" type="textBox" sizerel1024="10 10 390 65" textcolor="0 0 0" textfont="Arial nobold nodrop" textfontsize="12" textcentervert="">Remove %s from your friends list?</gadget>
    <button>OK</button>
    <button>$$18466$$Cancel</button>
    <command>gadgetUnreal("mpfriend-removedialog")</command>
  </gadget>
</layout>