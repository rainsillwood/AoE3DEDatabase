<layout>
  <gadget type="dialog" name="mpfriend-messagedialog" titlebar="A Friendly Message" size1024="0 0 400 130" bevel="" hidden="" a-dtype="textFontSize number Z number" centervert="" centerhoriz="" closebutton="">
    <gadget name="mpfriend-messagedialog-prompt" type="textBox" sizerel1024="10 10 390 30" textcolor="0 0 0" textfont="Arial nobold nodrop" textfontsize="12" textcentervert="">Message for %s</gadget>
    <gadget name="mpfriend-messagedialog-text" type="textField" sizerel1024="10 35 390 60" textcolor="0 0 0" textfont="Arial nodrop" textfontsize="12">Message:</gadget>
    <button>Send</button>
    <button>$$18466$$Cancel</button>
    <command>gadgetUnreal("mpfriend-messagedialog")</command>
  </gadget>
  <gadget type="dialog" name="mpfriend-viewmessagesdialog" titlebar="Message from %s" size1024="0 0 400 400" bevel="" hidden="" a-dtype="textFontSize number Z number" centervert="" centerhoriz="" closebutton="">
    <gadget name="mpfriend-viewmessagesdialog-text" type="textBox" sizerel1024="10 10 390 340" backgroundcolor="255 255 255" textcolor="0 0 0" textfont="Arial nodrop" textfontsize="12" textwrapon="" scrollvert="">
    </gadget>
    <button>Reply</button>
    <button>Done</button>
    <command>gadgetUnreal("mpfriend-viewmessagesdialog")</command>
  </gadget>
  <gadget type="dialog" name="mpfriend-adddialog" titlebar="Add Friend" size1024="0 0 400 130" bevel="" hidden="" a-dtype="textFontSize number Z number" centervert="" centerhoriz="" closebutton="">
